% test_Read_xls

close all
clear
%fs = matlab.io.datastore.FileSet("airlinesmall_subset.xlsx");
%ssds = spreadsheetDatastore("airlinesmall_subset.xlsx")
%ssds = spreadsheetDatastore("MecE260_UNDERGRAD Grade Dist Form.xlsx")
%sheetnames(ssds,1)
filename = "MecE260_UNDERGRAD Grade Dist Form.xlsx";
T = readtable(filename,'Sheet','SECOND YEAR', 'Format', 'auto');

data = struct;

for row = 3:4
    valueFlag = 0;
    for col = 1:size(T, 2)
        if (~strcmp(T.(col)(row), '')) && (mustBeNonNan())
            if valueFlag == 0
                field = T.(col)(row);
                field = strrep(field, ' ', '');
                field = strrep(field, ':', '');
                valueFlag = 1;
            elseif valueFlag == 1
                data.(field{1}) = T.(col)(row);
                valueFlag = 0;
            end     
        end
    end
end
